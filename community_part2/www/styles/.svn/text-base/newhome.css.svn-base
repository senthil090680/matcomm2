.home_ssbox{width:200px; height:195px; border:solid 7px #EDEDED;}
.home_ssbox1{width:200px; height:220px; border:solid 7px #EDEDED;}
/* Footer */
.footer{float:left; width:745px; padding:15px; background:#E2E2E2; margin:8px 0 0; position:relative;}
.footer1{float:left; width:745px; padding:15px; background:#E2E2E2; margin:8px 0 0;position:relative;}
