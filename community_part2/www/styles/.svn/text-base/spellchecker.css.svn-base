.status { color: #FF0000;padding-left: 150px;white-space: nowrap;}

.spell_container {}

.suggestion_box {
	background-color: #E4E4E4;
	border: 1px solid #000000;
	position: absolute;
	padding: 2px 2px 2px 2px;
	text-align: left;
}
.suggestion {
	cursor: pointer;
	text-decoration: none;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #000000;
}

.corrected_word
{
	color: #005500;
}

.addtoDictionary {
	background-color:  #E8F1FF;
	cursor: pointer;
	text-decoration: underline;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #000000;
}

.edit_box {
	background-color: #FFFFFF;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 11px;
	border: 1px inset #000000;
	padding-top: 2px;
	padding-left: 2px;
	overflow: auto;
}
.text_box {
	background-color: #FFFFFF;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 11px;
}
.highlight {
	color: #FF0000;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 11px;
	text-decoration: none;
	cursor: pointer;
}
.no_misspellings {
	color: #009933;
}
.check_spelling {font: normal 10px Tahoma; color: #FE7313; text-decoration: none;}
.resume_editing {
	color: #009933;
	text-decoration: none;
	cursor: pointer;
}